{
    "name": "reedware/domain-objects",
    "description": "Provides a clean service layer to generating domain objects.",
    "authors": [
        {
            "name": "Tyler Reed",
            "email": "tylernathanreed@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^10.13"
    },
    "require-dev": {
        "pestphp/pest": "^2.6",
        "mockery/mockery": "^1.5",
        "reedware/container-test-case": "^1.0",
        "reedware/is-attribute": "^1.0"
    },
    "repositories": [
        {"type": "path", "url": "../container-test-case"},
        {"type": "path", "url": "../is-attribute"}
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Reedware\\DomainObjects\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reedware\\DomainObjects\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
